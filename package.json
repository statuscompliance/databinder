{
  "name": "databinder",
  "version": "1.0.0",
  "description": "A TypeScript library to simplify integration with data sources",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
