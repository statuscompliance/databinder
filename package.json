    "@stylistic/eslint-plugin": "5.5.0",
    "@types/node": "24.9.2",
    "commitlint": "20.1.0",
    "eslint": "9.38.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@azure/identity": "4.10.1",
    "@microsoft/microsoft-graph-client": "3.0.7",
    "@opentelemetry/api": "1.9.0",
    "dotenv": "16.5.0",
